package com.mes.common.framework.Exception;

public class SlaveException extends Exception {
	/**
	 *
	 */
	private static final long serialVersionUID = 1L;

	public SlaveException(Exception e) {
		super(e);
	}

	public SlaveException(String msg, Exception e) {
		super(msg, e);
	}

	public SlaveException(String msg) {
		super(msg);
	}
}
